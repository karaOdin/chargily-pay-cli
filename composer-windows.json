{
    "name": "karaodin/chargily-pay-cli",
    "description": "Professional CLI for Chargily Pay - Algeria's leading payment gateway",
    "keywords": [
        "laravel",
        "laravel-zero",
        "chargily",
        "payment",
        "cli",
        "algeria",
        "fintech"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Chawki Mahdi",
            "email": "contact@devcloud.dz",
            "homepage": "https://devcloud.dz",
            "role": "CEO DevCloud, CTO Chargily"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/http": "^12.17",
        "illuminate/log": "^12.17",
        "laravel-zero/framework": "^12.0"
    },
    "suggest": {
        "ext-mbstring": "Required for proper string handling"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.2",
        "pestphp/pest-plugin-arch": "^3.1.1",
        "pestphp/pest-plugin-mutate": "^3.0.5",
        "ta-tikoma/phpunit-architecture-test": "^0.8.5",
        "brianium/paratest": "^7.8.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["chargily"]
}